{
  "mcpServers": {
    "ticktick": {
      "command": "/usr/local/bin/mcp-server-ticktick",
      "args": [],
      "env": {
        "TICKTICK_CLIENT_ID": "${TICKTICK_CLIENT_ID}",
        "TICKTICK_CLIENT_SECRET": "${TICKTICK_CLIENT_SECRET}",
        "TICKTICK_ACCESS_TOKEN": "${TICKTICK_ACCESS_TOKEN}"
      }
    },
    "planfix": {
      "command": "/usr/local/bin/planfix-mcp-server",
      "args": [],
      "env": {
        "PLANFIX_ACCOUNT": "${PLANFIX_ACCOUNT}",
        "PLANFIX_TOKEN": "${PLANFIX_TOKEN}"
      }
    },
    "google-calendar": {
      "command": "npx",
      "args": ["-y", "@cocal/google-calendar-mcp"],
      "env": {
        "GOOGLE_OAUTH_CREDENTIALS": "${GOOGLE_OAUTH_CREDENTIALS}"
      }
    }
  }
}
